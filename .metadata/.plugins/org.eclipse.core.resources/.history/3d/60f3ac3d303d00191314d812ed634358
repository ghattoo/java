package aritmetikai_operatorok;

public class aritm_Oper {

	public static void main(String[] args) {
		// aritmetikai operátorok (+ - * / %'modulo' ++ --)
		
		int szam1 = 7;
		int szam2 = 5;
		int szam3 = szam1 + szam2; //osszeadas
		int szam4 = szam1 - szam2; //kivonas
		
		System.out.println(szam3);
		System.out.println(szam4);
		// osszehasonlito operatorok (== > < != >= <= )
		
		// System.out.println("A");
		
		// logikai operatorok (&& || !)
		
		// System.out.println("A");
		
		//bitvájsz operátorok ()

	}

}
